    "version": "0.2.0",
    "configurations": [
        {
            "name": "operator",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "cmd/cluster-network-operator",
            "args": [
                "start",
                "--kubeconfig", "/root/git/dev-scripts/ocp/sriov/auth/kubeconfig",
            ],
            "env": {
                "OSDK_FORCE_RUN_MODE": "local",
                "SDN_IMAGE": "quay.io/pliurh/openshift-sdn:lm12", 
                "OVN_IMAGE": "quay.io/pliurh/ovn-kubernetes:ho39",
            },
            "cwd": "/root/git/cluster-network-operator",
            "envFile": "/tmp/env.sh"
        }
    ]
}